    </xsl:variable>
    <xsl:variable name="state" select="p:lookahead1(15, $input, $state)"/>          <!-- '{' -->
    <xsl:variable name="state" select="p:consume(64, $input, $state)"/>             <!-- '{' -->
    <xsl:variable name="state" select="p:parse-printcssquery-1($input, $state)"/>
