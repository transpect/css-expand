              </xsl:when>
              <xsl:when test="$state[$p:lk] != 12                                     (: IDENT :)
                          and $state[$p:lk] != 15                                     (: HASH :)
                          and $state[$p:lk] != 22                                     (: NOT :)
                          and $state[$p:lk] != 27                                     (: '(' :)
                          and $state[$p:lk] != 29                                     (: '*' :)
                          and $state[$p:lk] != 32                                     (: '.' :)
                          and $state[$p:lk] != 34                                     (: '/*' :)
                          and $state[$p:lk] != 35                                     (: ':' :)
                          and $state[$p:lk] != 38                                     (: '@' :)
                          and $state[$p:lk] != 44                                     (: '@page' :)
                          and $state[$p:lk] != 45                                     (: '[' :)
