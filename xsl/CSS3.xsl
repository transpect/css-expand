              else
                p:parse-and($input, $state)
            "/>
            <xsl:variable name="state" select="p:lookahead1(2, $input, $state)"/>   <!-- S -->
            <xsl:variable name="state" select="p:consume(6, $input, $state)"/>      <!-- S -->
