          <xsl:variable name="state" select="
            if ($state[$p:error]) then
              $state
            else
              p:parse-URL($input, $state)
          "/>
          <xsl:sequence select="$state"/>
        </xsl:when>
        <xsl:when test="$state[$p:l1] = 52">                                        <!-- 'attr(' -->
          <xsl:variable name="state" select="
            if ($state[$p:error]) then
              $state
            else
