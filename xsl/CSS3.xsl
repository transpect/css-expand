                      and $state[$p:l1] != 27                                         (: '(' :)
                      and $state[$p:l1] != 33                                         (: '.' :)
                      and $state[$p:l1] != 36                                         (: ':' :)
                      and $state[$p:l1] != 39                                         (: '@' :)
