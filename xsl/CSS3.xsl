    <xsl:variable name="state" select="p:consume(66, $input, $state)"/>             <!-- '}' -->
    <xsl:variable name="end" select="$state[$p:e0]"/>
    <xsl:sequence select="p:reduce($state, 'printcssquery', $count, $begin, $end)"/>
