        <xsl:choose>
          <xsl:when test="$state[$p:l1] != 6">                                      <!-- S -->
            <xsl:sequence select="$state"/>
          </xsl:when>
          <xsl:otherwise>
            <xsl:variable name="state" select="p:consume(6, $input, $state)"/>      <!-- S -->
            <xsl:sequence select="p:parse-fontquery-10($input, $state)"/>
          </xsl:otherwise>
        </xsl:choose>
      </xsl:otherwise>
    </xsl:choose>
  </xsl:function>

  <!--~
   ! Parse fontquery.
   !
   ! @param $input the input string.
   ! @param $state lexer state, error indicator, and result.
   ! @return the updated state.
  -->
  <xsl:function name="p:parse-fontquery" as="item()+">
    <xsl:param name="input" as="xs:string"/>
    <xsl:param name="state" as="item()+"/>

    <xsl:variable name="count" select="count($state)"/>
    <xsl:variable name="begin" select="$state[$p:e0]"/>
    <xsl:variable name="state" select="p:consume(40, $input, $state)"/>             <!-- '@font-face' -->
    <xsl:variable name="state" select="p:parse-fontquery-1($input, $state)"/>
    <xsl:variable name="state" select="p:consume(78, $input, $state)"/>             <!-- '{' -->
    <xsl:variable name="state" select="p:parse-fontquery-2($input, $state)"/>
    <xsl:variable name="state" select="p:parse-fontquery-3($input, $state)"/>
    <xsl:variable name="state" select="p:parse-fontquery-9($input, $state)"/>
    <xsl:variable name="state" select="p:parse-fontquery-10($input, $state)"/>
    <xsl:variable name="state" select="p:consume(80, $input, $state)"/>             <!-- '}' -->
    <xsl:variable name="end" select="$state[$p:e0]"/>
    <xsl:sequence select="p:reduce($state, 'fontquery', $count, $begin, $end)"/>
  </xsl:function>

  <!--~
   ! Parse the 1st loop of production simple_atrule (one or more). Use
   ! tail recursion for iteratively updating the lexer state.
   !
