                          and $state[$p:lk] != 46                                     (: '[' :)
                          and $state[$p:lk] != 65                                     (: '|' :)
                          and $state[$p:lk] != 774">                                <!-- S S -->
                <xsl:variable name="state" select="p:memoized($state, 6)"/>
